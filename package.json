{
  "name": "classless-component",
  "version": "1.0.6",
  "description": "A utility method to create react, preact, inferno components without using class keyword",
  "main": "./dist/index.js",
  "keywords": [
    "preact",
    "vdom",
    "react",
    "babel"
  ],
  "author": "staydecent",
  "license": "MIT",
  "homepage": "https://github.com/staydecent/classless-component#readme",
  "bugs": {
    "url": "https://github.com/staydecent/classless-component/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staydecent/classless-component.git"
  },
  "scripts": {
    "std": "standard --verbose | snazzy",
    "prebuild": "npm run std",
    "build": "babel index.js --out-dir dist",
    "pretest": "npm run std",
    "test": "mocha --compilers js:babel-register"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "inferno-component": "^3.0.3",
    "inferno-create-element": "^3.0.3",
    "inferno-server": "^3.0.3",
    "mocha": "^3.1.2",
    "preact": "^8.1.0",
    "preact-render-to-string": "^3.2.1",
    "react": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "snazzy": "^7.0.0",
    "standard": "^10.0.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-assign"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  }
}
